import java.util.Scanner;

public class SmallestElement {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

       
        int N = sc.nextInt();

       
        int smallest = Integer.MAX_VALUE;

        
        for (int i = 0; i < N; i++) {
            int num = sc.nextInt();
            if (num < smallest) {
                smallest = num;
            }
        }

        
        System.out.println(smallest);
    }
}
