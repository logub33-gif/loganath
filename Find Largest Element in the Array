import java.util.Scanner;

public class LargestElement {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        
        int N = sc.nextInt();

        
        int max = -100000; 
        
        for (int i = 0; i < N; i++) {
            int num = sc.nextInt();
            if (num > max) {
                max = num;
            }
        }

        
        System.out.println(max);

        sc.close();
    }
}
